{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2019"
    ],
    "types": [
      "babylonjs",
    //   "babylonjs-gui",
    //   "babylonjs-materials"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}
