{
  "name": "keeb.app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean-cdn": "aws s3 rm s3://keeb.app --recursive",
    "up-cdn": "aws s3 cp dist/. s3://keeb.app/ --recursive --acl public-read --delete",
    "up-x1": "aws s3 cp --cache-control max-age=0 ./dist/index.html s3://keeb.app/ --acl public-read",
    "up-x2": "aws s3 cp --cache-control max-age=0 ./dist/service-worker.js s3://keeb.app/ --acl public-read"
  },
  "dependencies": {
    "core-js": "^2.6.10",
    "moment": "^2.24.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.0.3",
    "vuetify": "^2.1.12"
  },
  "devDependencies": {
    "@mdi/js": "^4.6.95",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^5.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
